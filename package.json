{
  "name": "logbook-web",
  "version": "1.0.0",
  "description": "The web frontend for logbook-kai",
  "main": "index.tsx",
  "scripts": {
    "build:prod": "webpack --mode=production",
    "build:dev:watch": "webpack --mode=development --watch",
    "lint": "eslint --cache --ext .ts --ext .tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "git diff --staged --name-only --diff-filter=d | grep -E '\\.tsx?$' | xargs eslint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "npm run test -- --ci --runInBand",
    "test:watch": "npm run test -- --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "author": "Ryusuke SEKIYAMA",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@mdi/js": "^4.4.95",
    "@mdi/react": "^1.2.1",
    "@reduxjs/toolkit": "^1.1.0",
    "fecha": "^3.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.2",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^5.5.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
